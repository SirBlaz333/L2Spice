QWidget {
    background: white;
    color: black;
}

QTextEdit {
    border: 1px solid black;
}

QPushButton {
    padding: 5px 16px;
    font-size: 12px;
    font-weight: 500;
    line-height: 20px;
    vertical-align: middle;
    border: 1px solid;
    border-radius: 6px;
    color: #24292e;
}

QPushButton:pressed {
    border: 1px solid #1b1f2326;
    background-color: rgb(230, 30, 77);
}
